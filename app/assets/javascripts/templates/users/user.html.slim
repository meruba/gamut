.col-md-6.col-md-offset-3
  .user-information
    #header.row.text-center
      identicon size="100" username="vmUser.userData.email"
      h4 {{vmUser.userData.name}}
      .col-md-4.col-md-offset-4
        progresive-bar options="vmUser.optionsBar"
    #content.row
      .col-md-6.col-xs-6
        label {{ ::'user.attributes.email' | translate }}
        h5.text-muted {{vmUser.userData.email}}
      .text-right.col-md-6.col-xs-6
        label {{ ::'user.attributes.username' | translate }}
        h5.text-muted {{vmUser.userData.username}}
      .col-md-12
        div
          label {{ ::'user.attributes.address' | translate }}
          h5.text-muted {{vmUser.userData.address}}
        div
          label {{ ::'user.attributes.identification' | translate }}
          h5.text-muted {{vmUser.userData.identification}}
        div
          label {{ ::'user.attributes.telephone' | translate }}
          h5.text-muted {{vmUser.userData.telephone}}
